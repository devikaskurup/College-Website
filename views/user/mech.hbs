            {{/each}}

        </div>
    </div>
    <!-- question papers -->


    <div class="container">
        <h2 style="text-align: center;margin-top:50px">Question Papers</h2>
        <div class="row">
            {{#each questions}}
            <div class="column">
                <div class="card">
                    <p>{{semester}}, {{subject}},date:{{year_month}}</p>
                    <a href="/questions/{{_id}}.pdf" download="{{subject}} questionpaper{{year_month}}">click to
                        download</a>
                    <div class="FRAME">
                        <iframe scrolling="yes" src="/questions/{{_id}}.pdf">
                        </iframe>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>

    </div>
